                            <engine:Label text="Factory 1" class="factory-name" />
                            <engine:VisualElement class="factory-count">
                                <engine:Label text="Factory count :" class="text-label" />
                                <engine:Label text="5" class="count-label" />
                            </engine:VisualElement>
                        </engine:VisualElement>
                    </engine:VisualElement>
                    <engine:VisualElement class="management-section">
                        <engine:VisualElement class="current-productivity">
                            <engine:Label class="icon-label-current" />
